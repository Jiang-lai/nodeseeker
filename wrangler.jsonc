{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "nodeseeker",
	"compatibility_date": "2025-04-01",
	"compatibility_flags": [
		"nodejs_compat"
	],
  "main": "./src/index.tsx",
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "nodeseeker",
      "database_id": "a4e62e99-6ab4-4c9c-a53d-a4b4bd0d593c"
    }
  ],
  "triggers": {
    "crons": [
      "*/1 * * * *"
    ]
  },
  "observability": {
    "enabled": false
  },
	"assets": {
		"directory": "./dist",
		"not_found_handling": "single-page-application"
	},
}
